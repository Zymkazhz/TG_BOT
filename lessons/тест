from aiogram import Bot, Dispatcher, executor, types

TOKEN_API = '5688968215:AAFBDylGJtbA6sk_CbQhr8-d32u4yqk8t48'  # токен для подключения телеграм АПИ
PROXY_URL = 'socks5://45.185.6.35:7497'

bot = Bot(TOKEN_API, proxy=PROXY_URL)
dp = Dispatcher(bot)


@dp.message_handler()
async def echo(message: types.Message):
        await message.answer(text=f'Артем {message.text}')


if __name__ == '__main__':
    executor.start_polling(dp, skip_updates=True)